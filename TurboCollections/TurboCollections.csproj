<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>net5.0</TargetFramework>
        <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
        <PackageVersion>22.118.1</PackageVersion>
        <Title>Turbo Collections</Title>
        <Authors>Marc Zaku</Authors>
        <Description>Custom-Implemented Collections for research purposes.</Description>
        <Copyright>2022 (Marc Zaku)</Copyright>
        <PackageProjectUrl>https://github.com/marczaku/turbocollections</PackageProjectUrl>
        <PackageLicenseUrl>https://github.com/marczaku/TurboCollections/blob/master/LICENSE</PackageLicenseUrl>
        <RepositoryUrl>https://github.com/marczaku/turbocollections</RepositoryUrl>
        <RepositoryType>Git</RepositoryType>
        <Company>MarcZaku</Company>
        <AssemblyVersion>22.118.1</AssemblyVersion>
        <FileVersion>22.118.1</FileVersion>
        <NeutralLanguage>en-DE</NeutralLanguage>
        <SignAssembly>true</SignAssembly>
        <AssemblyOriginatorKeyFile>com.marczaku.turbocollections.snk</AssemblyOriginatorKeyFile>
        <PublicSign>true</PublicSign>
    </PropertyGroup>

</Project>
